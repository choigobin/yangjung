<?xml version= "1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
 <mapper namespace= "studentMapper">
 <insert id="insert">
INSERT INTO student( no
,name
,address
,hp
,tel )
VALUES( #{no}
, #{name}
, #{address}
, #{hp}
, #{tel} )
</insert>
 
 </mapper>